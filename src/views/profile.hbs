<div class="container-register">
  <ul class="profile-ul">
      <form >
    <li class="profile-item">
      First Name:
      {{userName}}
      <input class= "forform" value = {{userName}}>
    </li>
    <li class="profile-item">
      Last Name:
      {{userSurname}}
      <input class= "forform" value = {{userSurname}}>
    </li>
    <li class="profile-item">
      Email:
      {{userEmail}}
      <input class= "forform" value = {{userEmail}}>
    </li>
    
    
    </form>
  </ul>
  <ul class="fileurls">
    {{#each files}}
      <li>
        <a href="{{this.path}}">{{this.path}}</a>
      </li>
    {{/each}}
    </ul>

</div>

